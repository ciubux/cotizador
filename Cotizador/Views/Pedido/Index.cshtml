@{
    /**/

    ViewBag.Title = "Búsqueda Pedidos Venta";
    Model.Pedido pedido = ViewBag.pedido;
    Model.GuiaRemision guiaRemision = ViewBag.guiaRemision;
    List<Model.Pedido> pedidoList = ViewBag.pedidoList;
    Model.DocumentoVenta documentoVenta = ViewBag.documentoVenta;

    var allEmpresasClass = "text-muted";
    var allEmpresasChecked = "";

    if (pedido.integraEmpresas)
    {
        allEmpresasClass = "lbl-ajuste-calculo-precios";
        allEmpresasChecked = "checked";
    }


}

<div class="page-header">
    <h3>@ViewBag.Title</h3>
</div>
<div class="container">
    <input type="hidden" id="idPedido" value="@ViewBag.idPedido" />
    <input type="hidden" id="pagina" value="@ViewBag.pagina" />
    <input type="hidden" id="esCliente" value="@pedido.usuario.esCliente" />
    
    <form class="form-horizontal">

        <div class="row">
            <!--NÚMERO PEDIDO-->
            @if (!pedido.usuario.esCliente)
            {
                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        @Html.LabelFor(p => pedido.numeroPedido, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @if (pedido.numeroPedido > 0)
                            {
                                @Html.TextBoxFor(p => pedido.numeroPedido, htmlAttributes: new { @class = "form-control", type = "number", min = "0" })
                            }
                            else
                            {
                                @Html.TextBoxFor(p => pedido.numeroPedido, htmlAttributes: new { @class = "form-control", type = "number", min = "0", Value = "" })
                            }
                            <span style="margin-right: 20px; font-size:12px; color: darkred; font-weight: normal " class="has-error">*Si ingresa el número los otros criterios no serán considerados en la búsqueda.</span>
                        </div>
                    </div>

                </div>

                <!--NÚMERO GRUPO PEDIDO-->
                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        @Html.LabelFor(p => pedido.numeroGrupoPedido, htmlAttributes: new { @class = "control-label col-xs-4" })




                        <div class="col-xs-8">
                            @if (pedido.numeroGrupoPedido > 0)
                            {
                                @Html.TextBoxFor(p => pedido.numeroGrupoPedido, htmlAttributes: new { @class = "form-control", type = "number", min = "0" })
                            }
                            else
                            {
                                @Html.TextBoxFor(p => pedido.numeroGrupoPedido, htmlAttributes: new { @class = "form-control", type = "number", min = "0", Value = "" })
                            }
                        </div>

                    </div>
                </div>

                <!--CIUDAD-->
                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">

                        @Html.LabelFor(p => pedido.ciudad, htmlAttributes: new { @class = "control-label col-xs-4", @for = "idCiudad" })
                        <div class="col-xs-8">
                            @Html.Action("GetCiudades", "Ciudad",
                                                                                       new
                                                                                       {
                                                                                           ciudadSelectId = "idCiudad",
                                                                                           selectedValue = pedido.ciudad.idCiudad
                                                                                       })

                        </div>
                    </div>
                </div>

                <div class="col-sm-6 col-lg-4">
                    <!--Cliente-->
                    <div class="form-group">
                        <label for="idCliente" class="control-label col-xs-4">Cliente:</label>

                        <div class="col-xs-8">
                            <select class="form-control input-sm" id="idCliente" name="idCliente" required>
                                @if (ViewBag.existeCliente == 1)
                                {
                                    <option selected value="@pedido.cliente.idCliente">@pedido.cliente</option>
                                }
                                else
                                {
                                    <option value=""></option>
                                }
                            </select>
                        </div>
                    </div>

                </div>
            }
            else
            {

                <div class="col-sm-6 col-lg-4">
                    <!--USUARIO-->
                    <div class="form-group">
                        <label for="cliente" class="control-label col-xs-4">Cliente:</label>
                        <div class="col-xs-8">
                            @Html.Action("clientesPedidoList", "Cliente")
                        </div>
                    </div>
                </div>


                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        @Html.LabelFor(p => pedido.numeroReferenciaCliente, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @Html.TextBoxFor(p => pedido.numeroReferenciaCliente, htmlAttributes: new { @class = "form-control" })
                        </div>
                    </div>

                    <div class="form-group">
                        @Html.LabelFor(p => pedido.numeroRequerimiento, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @Html.TextBoxFor(p => pedido.numeroRequerimiento, htmlAttributes: new { @class = "form-control" })
                        </div>
                    </div>

                </div>

            }

        </div>


        @if (!pedido.usuario.esCliente)
        {
            <div class="row">
                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        @Html.LabelFor(p => pedido.numeroReferenciaCliente, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @Html.TextBoxFor(p => pedido.numeroReferenciaCliente, htmlAttributes: new { @class = "form-control" })
                        </div>
                    </div>

                    <div class="form-group">
                        @Html.LabelFor(p => pedido.numeroRequerimiento, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @Html.TextBoxFor(p => pedido.numeroRequerimiento, htmlAttributes: new { @class = "form-control" })
                        </div>
                    </div>

                </div>


                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        <label for="tipoPedido" class="control-label col-xs-4">Tipo Pedido:</label>
                        <div class="col-xs-8">
                            @Html.DropDownList("tipoPedidoBusqueda",
                             EnumHelper.GetSelectList(typeof(Model.Pedido.tiposPedidoVentaBusqueda), pedido.tipoPedidoVentaBusqueda),
                             new { @class = "form-control" }
                             )

                        </div>
                    </div>

                    <div class="form-group">
                        @Html.LabelFor(p => pedido.sku, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @Html.TextBoxFor(p => pedido.sku, htmlAttributes: new { @class = "form-control" })
                            <span style="margin-right: 20px; font-size:12px; color: darkred; font-weight: normal " class="has-error">*Puede ingresar el SKU del producto que se encuentra en el detalle del pedido.</span>
                        </div>

                    </div>

                </div>

                <!--CODIGO GRUPO CLIENTE-->
                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="pedido_idGrupoCliente">Grupo Cliente:</label>
                        <div class="col-xs-8">
                            @Html.Action("GetGruposCliente", "GrupoCliente",
                                    new
                                    {
                                        grupoClienteSelectId = "pedido_idGrupoCliente",
                                        selectedValue = pedido.idGrupoCliente
                                    })
                        </div>
                        @if (pedido.usuario.buscaSedesGrupoCliente)
                        {
                            <label class="radio-label" style="margin-left: 15px;margin-top: 5px;">
                                @if (pedido.buscarSedesGrupoCliente)
                                {
                                    <input checked type="checkbox" name="pedido_buscarSedesGrupoCliente" id="pedido_buscarSedesGrupoCliente" value="1">
                                }
                                else
                                {
                                    <input type="checkbox" name="pedido_buscarSedesGrupoCliente" id="pedido_buscarSedesGrupoCliente" value="1">
                                }
                                Todas las sedes
                            </label>
                        }
                    </div>

                    @*<div class="form-group">
                        @Html.LabelFor(p => pedido.truncado, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            <select id="truncado" class="form-control">
                                <option value="-1">Seleccione</option>
                                <option value="1">SI</option>
                                <option value="0">NO</option>
                            </select>
                        </div>

                    </div>*@
                </div>
            </div>
        }

        <div class="row">

            <!--FECHA CREACIÓN-->
            <div class="col-sm-6 col-lg-4">
                <label for="pedido_fechaCreacionDesde" class="control-label col-xs-6 col-lg-4">Fecha de Creación:</label>
                <div class="input-group col-xs-6 col-lg-8">
                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Desde:</span>
                    </div>
                    <input type="hidden" id="fechaCreacionDesdetmp" value="@ViewBag.fechaCreacionDesde" />
                    <input class="form-control" type="text" id="pedido_fechaCreacionDesde" name="pedido_fechaCreacionDesde" autocomplete="off">
                </div>
                <label for="fechaSolicitudHasta" class="control-label col-xs-6 col-lg-4"></label>
                <div class="input-group col-xs-6 col-lg-8">

                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Hasta:</span>
                    </div>
                    <input type="hidden" id="fechaCreacionHastatmp" value="@ViewBag.fechaCreacionHasta" />
                    <input class="form-control" type="text" id="pedido_fechaCreacionHasta" name="pedido_fechaCreacionHasta" autocomplete="off">

                </div>
            </div>

            <!--FECHA ENTREGA-->
            <div class="col-sm-6 col-lg-4">
                <label for="pedido_fechaEntregaDesde" class="control-label col-xs-6 col-lg-4">Fecha de Entrega:</label>
                <div class="input-group col-xs-6 col-lg-8">
                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Desde:</span>
                    </div>
                    <input type="hidden" id="fechaEntregaDesdetmp" value="@ViewBag.fechaEntregaDesde" />
                    <input class="form-control" type="text" id="pedido_fechaEntregaDesde" name="pedido_fechaEntregaDesde">
                </div>
                <label for="fechaEntregaHasta" class="control-label col-xs-6 col-lg-4"></label>
                <div class="input-group col-xs-6 col-lg-8">

                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Hasta:</span>
                    </div>
                    <input type="hidden" id="fechaEntregaHastatmp" value="@ViewBag.fechaEntregaHasta" />
                    <input class="form-control" type="text" id="pedido_fechaEntregaHasta" name="pedido_fechaEntregaHasta">

                </div>
            </div>

            <!--FECHA PROGRACION-->
            <div class="col-sm-6 col-lg-4">
                <label for="fechaEntregaHasta" class="control-label col-xs-6 col-lg-4">Fecha de Programación:</label>
                <div class="input-group col-xs-6 col-lg-8">
                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Desde:</span>
                    </div>
                    <input type="hidden" id="fechaProgramacionDesdetmp" value="@ViewBag.fechaProgramacionDesde" />
                    <input class="form-control" type="text" id="pedido_fechaProgramacionDesde" name="pedido_fechaProgramacionDesde">
                </div>
                <label for="fechaEntregaHasta" class="control-label col-xs-6 col-lg-4"></label>
                <div class="input-group col-xs-6 col-lg-8">

                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Hasta:</span>
                    </div>
                    <input type="hidden" id="fechaProgramacionHastatmp" value="@ViewBag.fechaProgramacionHasta" />
                    <input class="form-control" type="text" id="pedido_fechaProgramacionHasta" name="pedido_fechaProgramacionHasta">

                </div>
            </div>

        </div>





        <div class="row" style="margin-top: 10px">

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    <label for="estado" class="control-label col-xs-4">Estado Atención:</label>
                    <div class="col-xs-8">
                        @Html.DropDownList("estado",
                            EnumHelper.GetSelectList(typeof(Model.SeguimientoPedido.estadosSeguimientoPedido), pedido.seguimientoPedido.estado),
                            new { @class = "form-control" }
                            )

                        <label class="radio-label" style="margin-left: 15px;margin-top: 5px;">
                            @if (pedido.truncado == 1)
                            {
                                <input checked type="checkbox" name="pedido_buscarTruncados" id="pedido_buscarTruncados" value="1">
                            }
                            else
                            {
                                <input type="checkbox" name="pedido_buscarTruncados" id="pedido_buscarTruncados" value="1">
                            }
                            Incluir truncados
                        </label>
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    <label for="estadoCrediticio" class="control-label col-xs-4">Estado Crediticio:</label>
                    <div class="col-xs-8">
                        @Html.DropDownList("estadoCrediticio",
                                       EnumHelper.GetSelectList(typeof(Model.SeguimientoCrediticioPedido.estadosSeguimientoCrediticioPedido), pedido.seguimientoCrediticioPedido.estado),
                                       new { @class = "form-control" }
                                       )
                    </div>
                </div>
            </div>

            @if (!pedido.usuario.esCliente)
            {
                <div class="col-sm-6 col-lg-4">
                    <!--USUARIO-->
                    <div class="form-group">
                        <label for="usuario" class="control-label col-xs-4">Creado Por:</label>
                        <div class="col-xs-8">
                            @Html.Action("usuariosPedidoList", "Usuario")
                        </div>
                    </div>
                </div>
            }

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(c => pedido.responsableComercial, htmlAttributes: new { @class = "control-label col-xs-4", @for = "idResponsableComercial" })

                    <div class="col-xs-8">
                        @Html.Action("GetResponsablesComerciales", "Vendedor",
                                new
                                {
                                    vendedorSelectId = "idResponsableComercial",
                                    selectedValue = pedido.responsableComercial.idVendedor,
                                    disabled = pedido.usuario.modificaFiltroVendedor ? "" : "disabled"
                                })
                    </div>
                </div>
            </div>
        </div>

        @if (pedido.usuario.vistaIntegradaMultiEmpresa)
        {
        <div class="row" style="margin-top: 10px">
            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    <div class="col-xs-12">
                        <div class="onoffswitch" title="Considera todas las empresas en los resultados de búsqueda.">
                            <input type="checkbox" name="onoffswitch" @allEmpresasChecked class="onoffswitch-checkbox" id="chkIntegrarEmpresas">
                            <label class="onoffswitch-label" for="chkIntegrarEmpresas"></label>
                        </div>
                        <h5 id="lblChkIntegrarEmpresas" for="chkIntegrarEmpresas" style="width: 90%; font-size: 16px; margin-left: 45px; margin-top: -24px; font-weight:bold; cursor:pointer;" class="@allEmpresasClass" title="Considera todas las empresas en los resultados de búsqueda.">Integrar todas empresas</h5>
                    </div>
                </div>
            </div>
        </div>
        }

        <div class="row pull-right" style="margin-bottom:10px; margin-right:0px">
            <button id="btnLimpiarBusquedaPedidos" type="button" class="btn btn-primary  bouton-image botonClean">Limpiar</button>
            <button id="btnBusquedaPedidos" type="button" class="btn btn-primary  bouton-image botonSearch">Buscar</button>
        </div>

        <div id="divExportButton" class="row pull-left" style="margin-bottom:10px; margin-left:0px; display: none;">
            <button id="btnExportExcel" type="button" class="btn btn-primary  bouton-image botonExcel" actionLink="@Url.Action("ExportLastSearchExcel", "Pedido")">Exportar</button>
            @if (pedido.usuario.apruebaPedidos)
            {
                <button type="button" id="btnAprobarPedidosGrupo" class="btn btn-success" style="display: none;">Aprobar Pedidos Grupo</button>
            }
            @if (pedido.usuario.liberaPedidos)
            {
                <button type="button" id="btnLiberarPedidosGrupo" class="btn btn-success" style="display: none;">Liberar Pedidos Grupo</button>
            }
            @if (pedido.usuario.apruebaPedidos)
            {
                <button type="button" id="btnAprobarPedidosColectivo" class="btn btn-success" style="display: none;">Aprobar Pedidos Filtrados</button>
            }
            @if (pedido.usuario.liberaPedidos)
            {
                <button type="button" id="btnLiberarPedidosColectivo" class="btn btn-success" style="display: none;">Liberar Pedidos Filtrados</button>
            }
        </div>

        <table id="tablePedidos" class="table" data-editing="false" data-paging="true" data-paging-count-format="{CP} de {TP}"
               data-editing-show-text='<span class="fooicon fooicon-pencil" aria-hidden="true"></span> Editar Detalle'
               data-editing-allow-add="false"
               data-editing-allow-delete="false"
               data-editing-allow-edit="false"
               data-sorting="true"
               data-show-toggle="false"
               data-editing-hide-text="Finalizar edición"
               data-editing-add-text="Descartar cambios">
            <thead>
                <tr>
                    <th data-name="idPedido" data-visible="false"></th>
                    <th data-name="numero" data-visible="true">N°</th>
                    <th data-name="empresa" data-visible="true">Emp</th>
                    <th data-name="sedeMP" data-sortable="false" data-breakpoints="xs">Sede MP</th>
                    <th data-name="codigo" data-visible="true" data-sortable="false" data-breakpoints="xs">Cod. Cliente</th>
                    <th data-name="razonSocial" data-visible="true" data-sortable="true">Razón Social</th>
                    <th data-name="grupoCliente" data-visible="true" data-sortable="true">Grupo Cliente</th>
                    <th data-name="numeroReferenciaCliente" data-visible="true">O/C N°</th>
                    <th data-name="usuario" data-visible="true">Creado por</th>
                    <th data-name="fechaRegistro" data-sortable="true" data-breakpoints="xs">Fecha Registro</th>
                    <!--<th data-name="fechaSolicitud" data-sortable="true" data-breakpoints="xs">Fecha y Hora Solicitud</th>-->
                    <th data-name="rangoFechaEntrega" data-sortable="true" data-breakpoints="xs">Rango Fecha Entrega</th>
                    <th data-name="horarioEntrega" data-sortable="true" data-breakpoints="xs">Horarios Entrega</th>
                    <!--<th data-name="fechaProgramacion" data-sortable="true" data-breakpoints="xs">Programado Para:</th>-->
                    <th data-name="total" data-sortable="true" data-breakpoints="xs">Total (Incl. IGV)</th>
                    <th data-name="distrito" data-sortable="true" data-breakpoints="xs">Distrito Entrega</th>
                    <th data-name="estado" data-sortable="true" data-breakpoints="xs">Estado Atención</th>
                    <!--   <th data-name="aprobador" data-sortable="true" data-breakpoints="xs">Modificado última vez por</th>
                <th data-name="observacion" data-sortable="true" data-breakpoints="xs">Comentario Estado</th>-->
                    <th data-name="estadoCrediticio" data-sortable="true" data-breakpoints="xs">Estado Crediticio</th>
                    <th data-name="observaciones" data-sortable="true" data-breakpoints="xs">Obs. Uso Interno</th>

                    @if (pedido.usuario.confirmaStock)
                    {
                        <th data-name="stockConfirmado" data-visible="true">Stock</th>
                        <th data-name="stockConfirmado" data-visible="false">Stock</th>
                    }
                    else
                    {

                        <th data-name="stockConfirmado" data-visible="false">Stock</th>
                        <th data-name="stockConfirmado" data-visible="true">Stock</th>
                    }

                    <th data-name="opciones" data-sortable="true" data-breakpoints="xs"></th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>


        @if (pedidoList.Count == 0)
        {
            <div id="msgBusquedaSinResultados" style="display:none;" class="col-lg-12">
                <p class="pagination-centered" style=" font-size: medium" align="center">
                    La búsqueda no obtuvo resultados.
                </p>
            </div>
        }




    </form>
</div>


<div class="modal fade" id="modalVerPedido" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header" style="padding: 20px 15px;">
                <h4 class="modal-title" id="modalVerPedido">
                    <img src="~/images/logos/logo_MP.png" id="verImagenLogoPedido" height="35" style="margin-top: -8px; margin-left: 25px; margin-right: 5px;" />
                    <b>Información de Pedido Venta</b>
                </h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <p></p>
            </div>


            <div class="modal-body">
                <form class="form-horizontal">
                    <input type="hidden" id="verIdPedido" />
                    <input type="hidden" id="verIdCliente" />
                    <input type="hidden" id="verFechaEntregaExtendida" />
                    <!--NUMEROS PEDIDO-->
                    <div class="row">
                        <!--NÚMERO PEDIDO-->
                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(p => pedido.numeroPedido, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="verNumero"></p>
                                </div>
                            </div>
                        </div>
                        <!--NUMERO PEDIDO GRUPO
                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(p => pedido.numeroGrupoPedido, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="verNumeroGrupo"></p>
                                </div>
                            </div>
                        </div>-->
                        <!--NUMERO COTIZACION-->
                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(p => pedido.cotizacion.codigo, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="verCotizacionCodigo"></p>
                                    <button id="verLnkDFCotizacion" type="button" class="btn btn-link lnkAction lnkActionLabel lnkDFCotizacion">Descargar PDF</button>
                                </div>
                            </div>
                        </div>


                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(p => pedido.tipoPedido, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="verTipoPedido"></p>
                                </div>
                            </div>
                        </div>

                    </div>

                    <!--DATOS DEL CLIENTE-->
                    <div class="row">
                        <!--CIUDAD-->
                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(p => pedido.ciudad, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="verCiudad"></p>
                                </div>
                            </div>
                        </div>
                        <!--CLIENTE-->
                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
