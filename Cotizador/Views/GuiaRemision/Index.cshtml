                    <span aria-hidden="true">&times;</span>
                </button>
                <p></p>
            </div>

            <div class="modal-body">
                <form class="form-horizontal">


                    <!--GUIA REMISIÓN-->
                    <div class="row">
                        <div class="col-sm-6 col-lg-8">
                            <div class="form-group">
                                <label for="serieNumeroDocumentoParaNotaIngreso" class="control-label col-xs-4">Guía Remisión:</label>
                                <div class="col-xs-8">
                                    <input class="form-control" type="text" id="serieNumeroDocumentoParaNotaIngreso" disabled />
                                </div>
                            </div>
                        </div>
                    </div>

