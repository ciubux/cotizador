@{
    ViewBag.Title = "Búsqueda Guías Remisión";
    Model.GuiaRemision guiaRemision = ViewBag.guiaRemision;
    List<Model.GuiaRemision> guiaRemisionList = ViewBag.guiaRemisionList;
    Model.Pedido pedido = ViewBag.pedido;
    Model.DocumentoVenta documentoVenta = ViewBag.documentoVenta;
    Model.NotaIngreso notaIngreso = ViewBag.notaIngreso;
}

<div class="page-header">
    <h3>@ViewBag.Title</h3>
</div>
<div class="container">
    <input type="hidden" id="pagina" value="@ViewBag.pagina" />
    <input type="hidden" id="idClienteFacturacion" value="" />


    <form class="form-horizontal">

        <div class="row">
            <!--NÚMERO/SERIE GUIA-->
            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(p => guiaRemision.numeroDocumento, htmlAttributes: new { @class = "control-label col-xs-4" })

                    <div class="col-xs-8">
                        @if (guiaRemision.numeroDocumento > 0)
                        {
                            @Html.TextBoxFor(p => guiaRemision.numeroDocumento, htmlAttributes: new { @class = "form-control", type = "number", min = "0" })
                        }
                        else
                        {
                            @Html.TextBoxFor(p => guiaRemision.numeroDocumento, htmlAttributes: new { @class = "form-control", type = "number", min = "0", Value = "" })
                        }
                        <span style="margin-right: 20px; font-size:12px; color: darkred; font-weight: normal " class="has-error">*Si ingresa el número los otros criterios no serán considerados en la búsqueda.</span>
                    </div>

                </div>

            </div>

            <div class="col-sm-6 col-lg-4">
                <div class="row">

                    <!--NUMERO PEDIDO-->
                    <div class="col-sm-12 col-lg-12">
                        <div class="form-group">
                            @Html.LabelFor(p => guiaRemision.pedido.numeroPedido, htmlAttributes: new { @class = "control-label col-xs-4" })

                            <div class="col-xs-8">
                                @if (guiaRemision.pedido.numeroPedido > 0)
                                {
                                    @Html.TextBoxFor(p => guiaRemision.pedido.numeroPedido, htmlAttributes: new { @class = "form-control", type = "number", min = "0" })
                                }
                                else
                                {
                                    @Html.TextBoxFor(p => guiaRemision.pedido.numeroPedido, htmlAttributes: new { @class = "form-control", type = "number", min = "0", Value = "" })
                                }
                            </div>

                        </div>

                    </div>

                    <div class="col-sm-12 col-lg-12">
                        <div class="form-group">
                            @Html.LabelFor(p => guiaRemision.pedido.numeroGrupoPedido, htmlAttributes: new { @class = "control-label col-xs-4" })

                            <div class="col-xs-8">
                                @if (guiaRemision.pedido.numeroGrupoPedido > 0)
                                {
                                    @Html.TextBoxFor(p => guiaRemision.pedido.numeroGrupoPedido, htmlAttributes: new { @class = "form-control", type = "number", min = "0" })
                                }
                                else
                                {
                                    @Html.TextBoxFor(p => guiaRemision.pedido.numeroGrupoPedido, htmlAttributes: new { @class = "form-control", type = "number", min = "0", Value = "" })
                                }
                            </div>

                        </div>

                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(p => guiaRemision.estadoFiltro, htmlAttributes: new { @class = "control-label col-xs-4" })

                    <div class="col-xs-8">
                        @Html.DropDownList("estadoFiltro",
                                   EnumHelper.GetSelectList(typeof(Model.GuiaRemision.EstadoFiltro), guiaRemision.estadoFiltro),
                                   new { @class = "form-control" }
                                   )
                    </div>
                </div>

            </div>

        </div>


        <div class="row">
            <!--CIUDAD-->
            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(p => guiaRemision.ciudadOrigen, htmlAttributes: new { @class = "control-label col-xs-4", @for = "idCiudad" })
                    <div class="col-xs-8">
                        @Html.Action("GetCiudades", "Ciudad",
                                                                    new
                                                                    {
                                                                        ciudadSelectId = "idCiudad",
                                                                        selectedValue = guiaRemision.ciudadOrigen.idCiudad
                                                                    })
                    </div>
                </div>
            </div>
            <div class="col-sm-6 col-lg-4">
                <!--Cliente-->
                <div class="form-group">
                    <label for="idCliente" class="control-label col-xs-4">Cliente:</label>

                    <div class="col-xs-8">
                        <select class="form-control input-sm" id="idCliente" name="idCliente" required>
                            @if (ViewBag.existeCliente == 1)
                            {
                                <option selected value="@guiaRemision.pedido.cliente.idCliente">@guiaRemision.pedido.cliente</option>
                            }
                            else
                            {
                                <option value=""></option>
                            }
                        </select>
                    </div>
                </div>

            </div>

            <!--CODIGO GRUPO CLIENTE-->
            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    <label class="control-label col-xs-4" for="idGrupoCliente">Grupo Cliente:</label>
                    <div class="col-xs-8">
                        @Html.Action("GetGruposCliente", "GrupoCliente",
                                new
                                {
                                    grupoClienteSelectId = "idGrupoCliente",
                                    selectedValue = guiaRemision.pedido.idGrupoCliente
                                })
                    </div>
                    @if (guiaRemision.usuario.buscaSedesGrupoCliente)
                    {
                        <label class="radio-label" style="margin-left: 15px;margin-top: 5px;">
                            @if (guiaRemision.pedido.buscarSedesGrupoCliente)
                            {
                                <input checked type="checkbox" name="buscarSedesGrupoCliente" id="buscarSedesGrupoCliente" value="1">
                            }
                            else
                            {
                                <input type="checkbox" name="buscarSedesGrupoCliente" id="buscarSedesGrupoCliente" value="1">
                            }
                            Todas las sedes
                        </label>
                    }
                </div>
            </div>

        </div>



        <div class="row">
            <!--FECHA-->
            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(p => guiaRemision.fechaTrasladoDesde, htmlAttributes: new { @class = "control-label col-xs-4" })
                    <div class="col-xs-8">
                        <input type="hidden" id="fechaTrasladoDesdetmp" value="@ViewBag.fechaTrasladoDesde" />
                        <input class="form-control" type="text" id="guiaRemision_fechaTrasladoDesde" name="guiaRemision_fechaTrasladoDesde">
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(p => guiaRemision.fechaTrasladoHasta, htmlAttributes: new { @class = "control-label col-xs-4" })
                    <div class="col-xs-8">
                        <input type="hidden" id="fechaTrasladoHastatmp" value="@ViewBag.fechaTrasladoHasta" />
                        <input class="form-control" type="text" id="guiaRemision_fechaTrasladoHasta" name="guiaRemision_fechaTrasladoHasta">
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    <label for="motivoTraslado" class="control-label col-xs-4">Motivo Traslado:</label>
                    <div class="col-xs-8">
                        @Html.DropDownList("motivoTraslado",
                                EnumHelper.GetSelectList(typeof(Model.GuiaRemision.motivosTrasladoBusqueda), guiaRemision.motivoTrasladoBusqueda),
                                new { @class = "form-control" }
                                )
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-sm-6 col-lg-4">
                <div class="form-group row">
                    <label for="guiaRemision_estaFacturado" class="control-label col-xs-4">¿Considerar Facturados?:</label>
                    <div class="col-xs-8">
                        <div class="radio radio-inline">
                            <label class="radio-label">
                                @if (guiaRemision.estaFacturado)
                                {
                                    <input class="radio-input" checked type="radio" name="guiaRemision_estaFacturado" id="facturadosi" value="1">@ViewBag.Si;
                                }
                                else
                                {
                                    <input class="radio-input" type="radio" name="guiaRemision_estaFacturado" id="facturadosi" value="1">@ViewBag.Si;
                                }

                            </label>
                        </div>
                        <div class="radio radio-inline">
                            <label class="radio-label">
                                @if (!guiaRemision.estaFacturado)
                                {
                                    <input class="radio-input" checked type="radio" name="guiaRemision_estaFacturado" id="facturadono" value="0">@ViewBag.No;
                                }
                                else
                                {
                                    <input class="radio-input" type="radio" name="guiaRemision_estaFacturado" id="facturadono" value="0">@ViewBag.No;
                                }
                            </label>
                        </div>
                    </div>
                </div>
            </div>


            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    <label for="guiaRemision_estaAnulado" class="control-label col-xs-4">¿Considerar Anulados?:</label>
                    <div class="col-xs-8">
                        <div class="radio radio-inline">
                            <label class="radio-label">
                                @if (guiaRemision.estaAnulado)
                                {
                                    <input class="radio-input" checked type="radio" name="guiaRemision_estaAnulado" id="anuladosi" value="1">@ViewBag.Si;
                                }
                                else
                                {
                                    <input class="radio-input" type="radio" name="guiaRemision_estaAnulado" id="anuladosi" value="1">@ViewBag.Si;
                                }

                            </label>
                        </div>
                        <div class="radio radio-inline">
                            <label class="radio-label">
                                @if (!guiaRemision.estaAnulado)
                                {
                                    <input class="radio-input" checked type="radio" name="guiaRemision_estaAnulado" id="anuladono" value="0">@ViewBag.No;
                                }
                                else
                                {
                                    <input class="radio-input" type="radio" name="guiaRemision_estaAnulado" id="anuladono" value="0">@ViewBag.No;
                                }
                            </label>
                        </div>
                    </div>

                </div>
            </div>
        </div>



        <div class="row">
            <div class="col-sm-6 col-lg-4">
                <div class="form-group row">
                    <label for="guiaRemision_cpeNro" class="control-label col-xs-4">Número Factura venta:</label>
                    <div class="col-xs-8">
                        @Html.TextBoxFor(p => guiaRemision.cpeNro, htmlAttributes: new { @class = "form-control", type = "number", min = "0" })
                    </div>
                </div>
            </div>


            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(c => guiaRemision.responsableComercial, htmlAttributes: new { @class = "control-label col-xs-4", @for = "idResponsableComercial" })

                    <div class="col-xs-8">
                        @Html.Action("GetResponsablesComerciales", "Vendedor",
                                new
                                {
                                    vendedorSelectId = "idResponsableComercial",
                                    selectedValue = guiaRemision.responsableComercial.idVendedor,
                                    disabled = guiaRemision.usuario.modificaFiltroVendedor ? "" : "disabled"
                                })
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(p => guiaRemision.sku, htmlAttributes: new { @class = "control-label col-xs-4" })

                    <div class="col-xs-8">
                        @Html.TextBoxFor(p => guiaRemision.sku, htmlAttributes: new { @class = "form-control" })
                        <span style="margin-right: 20px; font-size:12px; color: darkred; font-weight: normal " class="has-error">*Puede ingresar el SKU del producto que se encuentra en el detalle del movimiento de almacén.</span>
                    </div>
                </div>
            </div>
        </div>



        <!--ESTADO-
    <div class="row">


        <div class="col-sm-6 col-lg-5">
            <div class="form-group">
                <label for="estado" class="control-label col-xs-4">Estado:</label>
                <div class="col-xs-8">
                    @Html.DropDownList("estado",
                             EnumHelper.GetSelectList(typeof(Model.SeguimientoMovimientoAlmacenSalida.estadosSeguimientoMovimientoAlmacenSalida), guiaRemision.seguimientoMovimientoAlmacenSalida.estado),
                             new { @class = "form-control" }
                             )
                </div>
            </div>
        </div>
    </div>-->

        <div class="row pull-right" style="margin-bottom:10px; margin-right:0px">
            <button id="btnLimpiarBusqueda" type="button" class="btn btn-primary  bouton-image botonClean">Limpiar</button>
            <button id="btnBusqueda" type="button" class="btn btn-primary  bouton-image botonSearch">Buscar</button>
        </div>

        <div id="divExportButton" class="row pull-left" style="margin-bottom:10px; margin-left:0px; display: none;">
            <button id="btnExportExcel" type="button" class="btn btn-primary  bouton-image botonExcel" actionLink="@Url.Action("ExportLastSearchExcel", "GuiaRemision")">Exportar</button>
        </div>

        <table id="tableGuiasRemision" class="table" data-editing="false" data-paging="true" data-paging-count-format="{CP} de {TP}"
               data-editing-show-text='<span class="fooicon fooicon-pencil" aria-hidden="true"></span> Editar Detalle'
               data-editing-allow-add="false"
               data-editing-allow-delete="false"
               data-editing-allow-edit="false"
               data-sorting="true"
               data-show-toggle="false"
               data-editing-hide-text="Finalizar edición"
               data-editing-add-text="Descartar cambios">
            <thead>
                <tr>
                    <th data-name="idMovimientoAlmacen" data-visible="false"></th>
                    <!--   <th data-name="numero" data-visible="true">N° Mov.</th>-->
                    <th data-name="numeroGuia" data-visible="true">N° Guía</th>
                    <th data-name="numeroPedido" data-visible="true">N° Pedido</th>
                    <th data-name="motivoTraslado" data-visible="true">Motivo Traslado</th>
                    <th data-name="usuario" data-visible="true">Creado por</th>
                    <!--  <th data-name="fecha" data-sortable="true">Fecha Creación</th>-->
                    <th data-name="fechaEmisión" data-sortable="true" data-breakpoints="xs">Fecha Emisión</th>
                    <th data-name="fechaTraslado" data-sortable="true" data-breakpoints="xs">Fecha Traslado</th>
                    <th data-name="razonSocial" data-sortable="true">Razón Social</th>
                    <th data-name="ruc" data-sortable="false" data-breakpoints="xs">RUC</th>
                    <th data-name="ciudad" data-sortable="false" data-breakpoints="xs">Sede MP</th>
                    <!--  <th data-name="subtotal" data-sortable="true" data-breakpoints="xs">Sub Total</th>
                <th data-name="igv" data-sortable="true" data-breakpoints="xs">IGV</th>-->
                    <th data-name="estado" data-sortable="true" data-breakpoints="xs">Estado</th>
                    <!--   <th data-name="aprobador" data-sortable="true" data-breakpoints="xs">Modificado última vez por</th>
                <th data-name="observacion" data-sortable="true" data-breakpoints="xs">Comentario Estado</th>-->
                    <th data-name="extornada" data-sortable="true" data-breakpoints="xs">¿Extornada?</th>
                    @if (guiaRemision.usuario.creaGuias)
                    {
                        <th data-name="noEntregado" data-visible="true">No Entregado</th>
                        <th data-name="noEntregado" data-visible="false">No Entregado</th>
                    }
                    @if (!guiaRemision.usuario.creaGuias)
                    {
                        <th data-name="noEntregado" data-visible="false">No Entregado</th>
                        <th data-name="noEntregado" data-visible="true">No Entregado</th>
                    }
                    <th data-name="opciones" data-sortable="true" data-breakpoints="xs"></th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>

        @if (guiaRemisionList.Count == 0)
        {
            <div id="msgBusquedaSinResultados" style="display:none;" class="col-lg-12">
                <p class="pagination-centered" style=" font-size: medium" align="center">
                    La búsqueda no obtuvo resultados.
                </p>
            </div>
        }



    </form>
</div>


<div class="modal fade" id="modalVerGuiaRemision" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="modalVerGuia"><b>Información de Guía Remisión</b></h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <p></p>
            </div>



            <div class="modal-body">
                <form class="form-horizontal">

                    <input type="hidden" id="idMovimientoAlmacen" value="@ViewBag.idMovimientoAlmacen" />
                    <input type="hidden" id="ver_guiaRemision_facturaUnica" value="" />
                    <input type="hidden" id="ver_guiaRemision_guiaAtiendePedido" value="" />
                    <input type="hidden" id="ver_guiaRemision_numeroGrupo" value="" />
                    <!--Sede-->
                    <div class="row">
                        <!--SEDE MP-->
                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(g => guiaRemision.ciudadOrigen, htmlAttributes: new { @class = "control-label col-xs-4", @for = "idCiudad" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="ver_guiaRemision_ciudadOrigen_nombre"></p>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6 col-lg-8">
                            <!--Dirección Origen-->
                            <div class="form-group">
                                @Html.LabelFor(g => guiaRemision.ciudadOrigen.direccionPuntoPartida, htmlAttributes: new { @class = "control-label col-xs-2" })
                                <div class="col-xs-10">
                                    <p class="form-control-static" id="ver_guiaRemision_ciudadOrigen_direccionPuntoPartida"></p>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!--Numero Movimiento-->
                    <div class="row">



                        <div class="col-sm-6 col-lg-4">
                            <!--fecha Movimiento-->
                            <div class="form-group">
                                @Html.LabelFor(g => guiaRemision.fechaTraslado, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="ver_guiaRemision_fechaTraslado"></p>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6 col-lg-4">
                            <!--Serie Numero Guia-->
                            <div class="form-group">
                                @Html.LabelFor(g => guiaRemision.serieDocumento, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="ver_guiaRemision_serieNumeroDocumento"></p>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6 col-lg-4">
                            <!--Estado-->
                            <div class="form-group">
                                <label for="ver_guiaRemision_estadoDescripcion" class="control-label col-xs-4">Estado:</label>
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="ver_guiaRemision_estadoDescripcion"></p>
                                </div>
                            </div>
                        </div>
                    </div>


                    <!--NUMEROS PEDIDO-->
                    <div class="row">
                        <div class="col-sm-6 col-lg-4">
                            <!--Numero Pedido-->
                            <div class="form-group">
                                <input type="hidden" id="idPedido" />
                                @Html.LabelFor(g => guiaRemision.pedido.numeroPedido, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="ver_guiaRemision_pedido_numeroPedido"></p>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6 col-lg-4">
                            <!-- Cliente-->
                            <div class="form-group">
                                @Html.LabelFor(g => guiaRemision.pedido.cliente, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="ver_guiaRemision_pedido_cliente"></p>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6 col-lg-4">
                            <!--Numero Referencia Cliente-->
                            <div class="form-group">
                                @Html.LabelFor(g => guiaRemision.pedido.numeroReferenciaCliente, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="ver_guiaRemision_pedido_numeroReferenciaCliente"></p>
                                </div>
                            </div>
                        </div>




                    </div>

                    <!--Motivo Traslado-->
                    <div class="row">

                        <div class="col-sm-6 col-lg-4">
                            <!--fecha Emisión-->
                            <div class="form-group">
                                @Html.LabelFor(g => guiaRemision.fechaEmision, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="ver_guiaRemision_fechaEmision"></p>
                                </div>
                            </div>
                        </div>

                        <!--MOTIVO DE TRASLADO-->
                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(g => guiaRemision.motivoTraslado, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="ver_guiaRemision_motivoTraslado"></p>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                <label for="guiaRemision_atencionParcial" class="control-label col-xs-4">Tipo de Atención:</label>
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="ver_guiaRemision_atencionParcial"></p>

                                </div>

                            </div>
                        </div>


                    </div>

                    <!--Direccion Entrega-->
                    <fieldset>
                        <legend id="divpedido_transportista">Entregar en:</legend>
                        <!--DESTINO-->
                        <div class="row">

                            <div class="col-sm-6 col-lg-4">
                                <!--Numero Pedido-->
                                <div class="form-group">
                                    @Html.LabelFor(p => guiaRemision.pedido.ubigeoEntrega, htmlAttributes: new { @class = "control-label col-xs-4" })
                                    <div class="col-xs-8">
                                        <p class="form-control-static" id="ver_guiaRemision_pedido_ubigeoEntrega"></p>
                                    </div>
                                </div>
                            </div>


                            <div class="col-sm-6 col-lg-8">
                                <!--Dirección Destino-->
                                <div class="form-group">
                                    @Html.LabelFor(g => guiaRemision.pedido.direccionEntrega.descripcion, htmlAttributes: new { @class = "control-label col-xs-2" })
                                    <div class="col-xs-10">
                                        <p class="form-control-static" id="ver_guiaRemision_pedido_direccionEntrega"></p>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </fieldset>

                    <!--Transportista-->
                    <fieldset>
                        <legend id="divpedido_transportista">Transporte:</legend>

                        <div class="row">
                            <!--TRANSPORTISTA ENTREGA -->
                            <div class="col-sm-6 col-lg-4">
