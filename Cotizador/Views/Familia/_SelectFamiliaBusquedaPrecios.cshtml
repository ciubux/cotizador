@model IEnumerable<Model.Familia>

@Html.DropDownList("familiaBusquedaPrecios",
new SelectList(Model, "nombre", "nombre", this.Session["familia"]),
new { @class = "form-control ", onchange = @"
$.ajax({
url: '/Home/updateFamilia',
type: 'POST',
dataType: 'JSON',
data: {
    familia: this.value
}
});
" })