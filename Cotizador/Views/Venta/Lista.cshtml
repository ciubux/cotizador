@{
    ViewBag.Title = "Venta - Búsqueda";
    Model.Venta venta = ViewBag.Venta;
    Model.Pedido pedido = ViewBag.pedido;
    Model.GuiaRemision guiaRemision = ViewBag.guiaRemision;



}

<div class="page-header">
    <h3>@ViewBag.Title</h3>
</div>
<div class="container">
    <input type="hidden" id="pagina" value="@ViewBag.pagina" />
    <input type="hidden" id="idClienteFacturacion" value="" />
    <form class="form-horizontal">

        <div class="row">

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(dv => venta.ciudad, htmlAttributes: new { @class = "control-label col-xs-4", @for = "idCiudad" })
                    <div class="col-xs-8">
                        @Html.Action("GetCiudades", "Ciudad",
                    new
                    {
                        ciudadSelectId = "idCiudadBusquedaList",
                        selectedValue = venta.guiaRemision.ciudadOrigen.idCiudad
                    })
                    </div>
                </div>
            </div>


            <div class="col-sm-6 col-lg-4">
                <!--CLIENTE-->
                <div class="form-group">
                    <label for="idCliente" class="control-label col-xs-4">Cliente:</label>

                    <div class="col-xs-8">
                        <select class="form-control input-sm" id="idClienteBusquedaList" name="idCliente" required>
                            @if (ViewBag.existeCliente == 1)
                            {
                                <option selected value="@venta.guiaRemision.pedido.cliente.idCliente">@venta.guiaRemision.pedido.cliente</option>
                            }
                            else
                            {
                                <option value=""></option>
                            }
                        </select>
                    </div>
                </div>

            </div>

        </div>

        <div class="row">

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    <label for="documentoVenta_numero" class="control-label col-xs-4">Tipo/Número Doc.:</label>
                    <div class="col-xs-4">
                        @Html.DropDownList("VentaList_tipoDocumento",
                                          EnumHelper.GetSelectList(typeof(Model.DocumentoVenta.TipoDocumento), venta.documentoVenta.tipoDocumento),
                                          new { @class = "form-control" }
                                          )
                    </div>
                    <div class="col-xs-4">
                        @if (Int64.Parse(venta.documentoVenta.numero) > 0)
                        {
                            @Html.TextBoxFor(dv => venta.documentoVenta.numero, htmlAttributes: new { @class = "form-control", @id = "VentaList_numeroDocumento", type = "number", min = "0" })
                        }
                        else
                        {
                            @Html.TextBoxFor(dv => venta.documentoVenta.numero, htmlAttributes: new { @class = "form-control", @id = "VentaList_numeroDocumento", type = "number", min = "0", Value = "" })
                        }
                    </div>
                </div>
            </div>


            <!--NUMERO GUIA-->
            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    <label for="documentoVenta_guiaRemision_numero" class="control-label col-xs-4">Número Mov. Almacén:</label>

                    <div class="col-xs-8">
                        @if (venta.guiaRemision.numero > 0)
                        {
                            @Html.TextBoxFor(dv => venta.guiaRemision.numero, htmlAttributes: new { @class = "form-control", type = "number", min = "0" ,@id="venta_guiaRemision_numero_list" })
                        }
                        else
                        {
                            @Html.TextBoxFor(dv => venta.guiaRemision.numero, htmlAttributes: new { @class = "form-control", type = "number", min = "0", Value = "" ,@id="venta_guiaRemision_numero_list"})
                        }
                        <!--<span style="margin-right: 20px; font-size:12px; color: darkred; font-weight: normal " class="has-error">*Si ingresa el número los demás criterios no serán considerados al realizar la búsqueda.</span>-->
                    </div>

                </div>
            </div>




            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    <label for="documentoVenta_numero" class="control-label col-xs-4">Número Pedido:</label>

                    <div class="col-xs-8">
                        @if (venta.pedido.numeroPedido > 0)
                        {
                            @Html.TextBoxFor(dv => venta.pedido.numeroPedido, htmlAttributes: new { @class = "form-control", type = "number", min = "0" })
                        }
                        else
                        {
                            @Html.TextBoxFor(dv => venta.pedido.numeroPedido, htmlAttributes: new { @class = "form-control", type = "number", min = "0", Value = "" })
                        }
                        <!--<span style="margin-right: 20px; font-size:12px; color: darkred; font-weight: normal " class="has-error">*Si ingresa el número los demás criterios no serán considerados al realizar la búsqueda.</span>---->
                    </div>

                </div>
            </div>

        </div>

        <div class="row">
            <!--FECHA-->
            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(dv => venta.guiaRemision.fechaEmisionDesde, htmlAttributes: new { @class = "control-label col-xs-4" })
                    <div class="col-xs-8">
                        <input type="hidden" id="fechaEntregaDesdetmp" value="@ViewBag.fechaEmisionDesde" />
                        <input class="form-control" type="text" id="Venta_fechaEmision_Desde" name="Venta_fechaEmision_Desde" value="@ViewBag.venta.guiaRemision.fechaEmisionDesde">
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(dv => venta.guiaRemision.fechaEmisionHasta, htmlAttributes: new { @class = "control-label col-xs-4" })
                    <div class="col-xs-8">
                        <input type="hidden" id="fechaEntregaHastatmp" value="@ViewBag.fechaEmisionHasta" />
                        <input class="form-control" type="text" id="Venta_fechaEmision_Hasta" name="Venta_fechaEmision_Hasta" value="@ViewBag.venta.guiaRemision.fechaEmisionHasta">
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(p => venta.guiaRemision.sku, htmlAttributes: new { @class = "control-label col-xs-4" })

                    <div class="col-xs-8">
                        @Html.TextBoxFor(p => venta.guiaRemision.sku, htmlAttributes: new { @class = "form-control" })
                        <span style="margin-right: 20px; font-size:12px; color: darkred; font-weight: normal " class="has-error">*Puede ingresar el SKU del producto que se encuentra en el detalle del documento.</span>
                    </div>


                </div>

            </div>



        </div>


        <div class="row pull-right" style="margin-bottom:10px; margin-right:15px">
            <button id="btnLimpiarBusquedaVentaList" type="button" class="btn btn-primary  bouton-image botonClean">Limpiar</button>
            <button id="btnBusquedaVentaList" type="button" class="btn btn-primary  bouton-image botonSearch">Buscar</button>
        </div>



        <table id="tableVentaList" class="table" data-editing="false" data-paging="true" data-paging-count-format="{CP} de {TP}"
               data-editing-show-text='<span class="fooicon fooicon-pencil" aria-hidden="true"></span> Editar Detalle'
               data-editing-allow-add="false"
               data-editing-allow-delete="false"
               data-editing-allow-edit="false"
               data-sorting="true"
               data-show-toggle="false"
               data-editing-hide-text="Finalizar edición"
               data-editing-add-text="Descartar cambios">

            <thead>
                <tr>
                    <th data-name="idDocumentoVenta" data-visible="false"></th> 
                    <th data-name="numeroPedido" data-visible="true">Pedido</th>
                    <th data-name="serieNumeroGuia" data-visible="true">Movimiento Almacén</th>
                    <th data-name="serieNumeroFactura" data-visible="true">Factura/Boleta</th>
                    <!--<th data-name="usuario" data-visible="true">Creado Por</th> --!>
    <th data-name="fechaEmision" data-sortable="true" data-breakpoints="xs">Fecha Emisión</th>
    <!--<th data-name="fechaVencimiento" data-sortable="true" data-breakpoints="xs">Fecha Vencimiento</th>-->
                    <th data-name="codigoCliente" data-sortable="true">Cod. Cliente</th>
                    <th data-name="razonSocial" data-sortable="true">Razón Social</th>
                    <th data-name="ruc" data-sortable="false" data-breakpoints="xs">RUC</th>
                    <th data-name="sedeMP" data-sortable="false" data-breakpoints="xs">Sede MP</th>
                    <th data-name="total" data-sortable="true" data-breakpoints="xs">Total</th>
                    <th data-name="asesorComercial" data-sortable="true" data-breakpoints="xs">Asesor Comercial</th>
                    <!--<th data-name="estado" data-sortable="true" data-breakpoints="xs">Estado</th>-->
                    <th data-name="opciones" data-sortable="false" data-breakpoints="xs"></th>

                </tr>
            </thead>


            <tbody></tbody>
        </table>
    </form>

    <div class="modal fade" id="modalFacturar" tabindex="-1">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="modalFacturarTitle"><b>Datos de Venta</b></h4>
