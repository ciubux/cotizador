                                        @Html.LabelFor(dv => venta.documentoVenta.fechaEmision, htmlAttributes: new { @class = "control-label col-xs-4 col-lg-4" })
                                        <div class="col-xs-8">
                                            <!--<input type="hidden" id="fechaEmision" value="" />
                                            <input type="hidden" id="fechaEntregaHastaProgramacion" value="" />-->
                                            <input type="hidden" id="documentoVenta_fechaEmisiontmp" value="@ViewBag.fechaEmision" />
                                            <input class="form-control" type="text" id="documentoVenta_fechaEmisionList" name="documentoVenta_fechaEmisionList">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6 col-lg-4">
                                    <div class="form-group">
                                        @Html.LabelFor(dv => venta.documentoVenta.horaEmision, htmlAttributes: new { @class = "control-label col-xs-4 col-lg-4" })
                                        <div class="col-xs-8">
                                            <input class="form-control" type="text" id="documentoVenta_horaEmisionList" name="documentoVenta_horaEmisionList" value="@ViewBag.horaEmision">
                                        </div>
                                    </div>
                                </div>

                            </div>



                            <div class="row">
                                <div class="col-sm-6 col-lg-4">
                                    <div class="form-group">
                                        @Html.LabelFor(dv => venta.documentoVenta.tipoPago, htmlAttributes: new { @class = "control-label col-xs-4 col-lg-4" })
                                        <div class="col-xs-8">
                                            @Html.DropDownList("tipoPagoList",
                                                       EnumHelper.GetSelectList(typeof(Model.DocumentoVenta.TipoPago), venta.documentoVenta.tipoPago),
                                                       new { @class = "form-control" }
                                                       )
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6 col-lg-4">
                                    <div class="form-group">
                                        @Html.LabelFor(dv => venta.documentoVenta.formaPago, htmlAttributes: new { @class = "control-label col-xs-4 col-lg-4" })
                                        <div class="col-xs-8">
                                            @Html.DropDownList("formaPagoList",
                                                       EnumHelper.GetSelectList(typeof(Model.DocumentoVenta.FormaPago), venta.documentoVenta.formaPago),
                                                       new { @class = "form-control" }
                                                       )
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-6 col-lg-4">
                                    <div class="form-group">
                                        @Html.LabelFor(dv => venta.documentoVenta.fechaVencimiento, htmlAttributes: new { @class = "control-label col-xs-4 col-lg-4" })
                                        <div class="col-xs-8">
                                            <!--<input type="hidden" id="fechaEmision" value="" />
                                            <input type="hidden" id="fechaEntregaHastaProgramacion" value="" />-->
                                            <input type="hidden" id="documentoVenta_fechaVencimientotmp" value="" />
                                            <input class="form-control" type="text" id="documentoVenta_fechaVencimientoList" name="documentoVenta_fechaVencimientoList">
                                        </div>
                                    </div>
                                </div>


                            </div>


                        </fieldset>






                        <table id="tableDetallePedido" class="table" data-editing="false"
                               data-editing-show-text='<span class="fooicon fooicon-pencil" aria-hidden="true"></span> Editar Detalle'
                               data-editing-allow-add="false"
                               data-editing-allow-delete="false"
