                                </div>


                            </div>



                            <div class="row">
                                <!--CIUDAD-->
                                <div class="col-sm-6 col-lg-4">
                                    <div class="form-group">
                                        @Html.LabelFor(p => pedido.ciudad, htmlAttributes: new { @class = "control-label col-xs-4" })
                                        <div class="col-xs-8">
                                            <p class="form-control-static" id="verCiudad"></p>
                                        </div>
                                    </div>
                                </div>
                                <!--CLIENTE-->
                                <div class="col-sm-6 col-lg-4">
                                    <div class="form-group">
                                        @Html.LabelFor(p => pedido.cliente, htmlAttributes: new { @class = "control-label col-xs-4" })
                                        <div class="col-xs-8">
                                            <p class="form-control-static" id="verCliente"></p>
                                        </div>
                                    </div>
                                </div>


                                <div class="col-sm-6 col-lg-4">
                                    <div class="form-group">
                                        @Html.LabelFor(p => pedido.cliente.codigo, htmlAttributes: new { @class = "control-label col-xs-4" })
                                        <div class="col-xs-8">
                                            <p class="form-control-static" id="verClienteCodigo"></p>
                                        </div>
                                    </div>
                                </div>





                            </div>







                            <div class="row">
                                <!--NUMERO REFERENCIA CLIENTE-->
                                <div class="col-sm-6 col-lg-4" id="divReferenciaCliente">
                                    <div class="form-group">
                                        @Html.LabelFor(p => pedido.numeroReferenciaCliente, htmlAttributes: new { @class = "control-label col-xs-4" })
                                        <div class="col-xs-8">
                                            <p class="form-control-static" id="verNumeroReferenciaCliente"></p>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-6 col-lg-4" id="divCiudadSolicitante">
                                    <div class="form-group">
                                        <label for="estado" class="control-label col-xs-4">Sede MP Solicitante:</label>
                                        <div class="col-xs-8">
                                            <p class="form-control-static" id="verCiudadSolicitante"></p>
                                        </div>
                                    </div>
                                </div>

                                <!--NUMERO REFERENCIA ADICIONAL-->
                                <div class="col-sm-6 col-lg-4">
                                    <div class="form-group">
                                        @Html.LabelFor(p => pedido.numeroReferenciaAdicional, htmlAttributes: new { @class = "control-label col-xs-4" })
                                        <div class="col-xs-8">
                                            <p class="form-control-static" id="verNumeroReferenciaAdicional"></p>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">

                                <!--OBSERVACIOENS DEL PEDIDO-->
                                <div class="col-sm-6 col-lg-8">
                                    <div class="form-group">
                                        @Html.LabelFor(g => pedido.observaciones, htmlAttributes: new { @class = "control-label col-xs-2", style = "color: darkred" })
                                        <div class="col-xs-10">
                                            <p class="form-control-static" id="verObservacionesPedido"></p>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-6 col-lg-4">
                                    <div class="form-group">
                                        <label for="archivoAdjunto" class="control-label col-xs-4">Archivos Adjuntos:</label>
                                        <div class="col-xs-8">
                                            <ul id="nombreArchivos"></ul>
                                        </div>
                                    </div>
                                </div>

                            </div>







                        </fieldset>

                        <fieldset>
                            <legend id="divpedido_direccionEntrega">Datos de la Guía Remisión:</legend>

                            <div class="row">

                                <div class="col-sm-6 col-lg-4">
                                    <!--Serie Numero Guia-->
                                    <div class="form-group">
                                        @Html.LabelFor(g => guiaRemision.serieDocumento, htmlAttributes: new { @class = "control-label col-xs-4" })
                                        <div class="col-xs-8">
                                            <p class="form-control-static" id="facturarver_guiaRemision_serieNumeroDocumento"></p>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-6 col-lg-4">
                                    <!--fecha Emisión-->
                                    <div class="form-group">
                                        @Html.LabelFor(g => guiaRemision.fechaEmision, htmlAttributes: new { @class = "control-label col-xs-4" })
                                        <div class="col-xs-8">
                                            <p class="form-control-static" id="facturarver_guiaRemision_fechaEmision"></p>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-sm-6 col-lg-4">
                                    <!--fecha Movimiento-->
                                    <div class="form-group">
                                        @Html.LabelFor(g => guiaRemision.fechaTraslado, htmlAttributes: new { @class = "control-label col-xs-4" })
                                        <div class="col-xs-8">
                                            <p class="form-control-static" id="facturarver_guiaRemision_fechaTraslado"></p>
                                        </div>
                                    </div>
                                </div>

                            </div>

                            <!--DIRECCION ENTREGA-->
                            <div class="row">

                                <div class="col-sm-6 col-lg-4">
                                    <div class="form-group">
                                        @Html.LabelFor(p => pedido.ubigeoEntrega, htmlAttributes: new { @class = "control-label col-xs-4" })
                                        <div class="col-xs-8">
                                            <p class="form-control-static" id="verUbigeoEntrega"></p>
                                        </div>
                                    </div>
                                </div>

                                <!--DIRECCION ENTREGA -->
                                <div class="col-sm-6 col-lg-8">
                                    <div class="form-group">
                                        @Html.LabelFor(p => pedido.direccionEntrega.descripcion, htmlAttributes: new { @class = "control-label col-xs-2" })
                                        <div class="col-xs-10">
                                            <p class="form-control-static" id="verDireccionEntrega"></p>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </fieldset>



