@{
    ViewBag.Title = "Carga Masiva Precios Especiales";
    Model.PrecioEspecialCabecera obj = ViewBag.obj;
}

<div class="page-header">
    <h3>@ViewBag.Title</h3>
</div>

<div class="container">
    <input type="hidden" id="pagina" value="@ViewBag.pagina" />
    <input type="hidden" value="@ViewBag.debug" />
    <div class="form-horizontal">
        

        <div class="row">
            <form action="/PrecioEspecial/LoadAll" method="post" enctype="multipart/form-data" id="formCargarPrecios">
                <div class="col-xs-8 col-lg-4">
                    <!--Archivo-->
                    <label class="control-label col-xs-4" for="file">Cargar Archivo de Precios:</label>
                    <div class="col-xs-8">
                        <input class="form-control" type="file" name="file" id="file" accept="application/vnd.ms-excel" />
                    </div>
                </div>
                <div class="col-xs-4 col-lg-2">
                    <button id="btnCargarPrecios" type="submit" class="btn btn-primary">Cargar Precios</button>
                </div>
            </form>
        </div>


    </div>
</div>
