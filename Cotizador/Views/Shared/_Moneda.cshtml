@model Cotizador.Models.MonedaViewModels

@if (Model.Disabled)
{
    @Html.DropDownList(
                Model.SelectId,
                Model.Monedas, "Seleccione",
                new
                {
                    id = Model.SelectId,
                    @class = "form-control",
                    @disabled = "disabled"
                })
}
else
{
    @Html.DropDownList(
            Model.SelectId,
            Model.Monedas, "Seleccione",
            new
            {
                id = Model.SelectId,
                @class = "form-control"

            })
}





