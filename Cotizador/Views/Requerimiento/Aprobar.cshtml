                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(p => requerimiento.numeroReferenciaAdicional, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="verNumeroReferenciaAdicional"></p>
                                </div>
                            </div>
                        </div>
                    </div>

