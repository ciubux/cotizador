@{

    Model.Requerimiento requerimiento = ViewBag.requerimiento;
    List<Model.Requerimiento> requerimientoList = ViewBag.requerimientoList;
    Model.DocumentoVenta documentoVenta = ViewBag.documentoVenta;
    /**/
    if (requerimiento.usuario.idUsuario == Guid.Parse("7D485AFF-AC89-462D-BBB8-0F8ADF8C02F1"))
    {
        ViewBag.Title = "Aprobar Requerimientos";
    }
    else {
        ViewBag.Title = "Requerimientos";
    }
}

<div class="page-header">
    <h3>@ViewBag.Title</h3>
</div>
<div class="container">
    <input type="hidden" id="idRequerimiento" value="@ViewBag.idRequerimiento" />
    <input type="hidden" id="pagina" value="@ViewBag.pagina" />
    <input type="hidden" id="esCliente" value="@requerimiento.usuario.esCliente" />

    <form class="form-horizontal">

        <div class="row">
            <!--NÚMERO PEDIDO-->
            @if (requerimiento.usuario.esCliente)
            {
                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        <label for="numeroSolicitud" class="control-label col-xs-4">Número Requerimiento:</label>

                        <div class="col-xs-8">
                            @if (requerimiento.idRequerimiento > 0)
                            {
                                @Html.TextBoxFor(p => requerimiento.idRequerimiento, htmlAttributes: new { @class = "form-control", type = "number", min = "0" })
                            }
                            else
                            {
                                @Html.TextBoxFor(p => requerimiento.idRequerimiento, htmlAttributes: new { @class = "form-control", type = "number", min = "0", Value = "" })
                            }
                            <span hidden="hidden" style="margin-right: 20px; font-size:12px; color: darkred; font-weight: normal " class="has-error">*Si ingresa el número los otros criterios no serán considerados en la búsqueda.</span>
                        </div>
                    </div>

                </div>

                <div class="col-sm-6 col-lg-4" id="divPeriodo">
	                <div class="form-group">
		                @Html.LabelFor(p => requerimiento.periodo, htmlAttributes: new { @class = "control-label col-xs-4", @for = "idPeriodo" })
		                <div class="col-xs-8">
			                @Html.Action("GetPeriodosView", "PeriodoSolicitud",
													                new
													                {
														                periodoSelectId = "idPeriodo",
														                selectedValue = requerimiento.periodo.idPeriodoSolicitud,
                                                                        excluirPeriodosConRequerimientos = false
													                })
		                </div>
	                </div>
                </div>





                <!--CIUDAD-->
                <div hidden="hidden" class="col-sm-6 col-lg-4">
                    <div class="form-group">

                        @Html.LabelFor(p => requerimiento.ciudad, htmlAttributes: new { @class = "control-label col-xs-4", @for = "idCiudad" })
                        <div class="col-xs-8">
                            @Html.Action("GetCiudades", "Ciudad",
                                                                       new
                                                                       {
                                                                           ciudadSelectId = "idCiudad",
                                                                           selectedValue = requerimiento.ciudad.idCiudad
                                                                       })

                        </div>
                    </div>
                </div>

                <div hidden="hidden" class="col-sm-6 col-lg-4">
                    <!--Cliente-->
                    <div class="form-group">
                        <label for="idCliente" class="control-label col-xs-4">Cliente:</label>

                        <div class="col-xs-8">
                            <select class="form-control input-sm" id="idCliente" name="idCliente" required>
                                @if (ViewBag.existeCliente == 1)
                                {
                                    <option selected value="@requerimiento.cliente.idCliente">@requerimiento.cliente</option>
                                }
                                else
                                {
                                    <option value=""></option>
                                }
                            </select>
                        </div>
                    </div>

                </div>
            }
            else
            {

                <div class="col-sm-6 col-lg-4">
                    <!--USUARIO-->
                    <div class="form-group">
                        <label for="cliente" class="control-label col-xs-4">Cliente:</label>
                        <div class="col-xs-8">
                            @Html.Action("clientesPedidoList", "Cliente")
                        </div>
                    </div>
                </div>


                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        @Html.LabelFor(p => requerimiento.numeroReferenciaCliente, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @Html.TextBoxFor(p => requerimiento.numeroReferenciaCliente, htmlAttributes: new { @class = "form-control" })
                        </div>
                    </div>

                    <div class="form-group">
                        @Html.LabelFor(p => requerimiento.idRequerimiento, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @Html.TextBoxFor(p => requerimiento.idRequerimiento, htmlAttributes: new { @class = "form-control" })
                        </div>
                    </div>

                </div>

            }

        </div>


        @if (!requerimiento.usuario.esCliente)
        {
            <div hidden="hidden" class="row">
                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        @Html.LabelFor(p => requerimiento.numeroReferenciaCliente, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @Html.TextBoxFor(p => requerimiento.numeroReferenciaCliente, htmlAttributes: new { @class = "form-control" })
                        </div>
                    </div>

                    <div class="form-group">
                        @Html.LabelFor(p => requerimiento.idRequerimiento, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @Html.TextBoxFor(p => requerimiento.idRequerimiento, htmlAttributes: new { @class = "form-control" })
                        </div>
                    </div>

                </div>


                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        <label for="tipoPedido" class="control-label col-xs-4">Tipo Pedido:</label>
                        <div class="col-xs-8">
                            @Html.DropDownList("tipoPedidoBusqueda",
             EnumHelper.GetSelectList(typeof(Model.Requerimiento.tiposRequerimientoVentaBusqueda), requerimiento.tipoRequerimientoVentaBusqueda),
             new { @class = "form-control" }
             )

                        </div>
                    </div>

                    <div class="form-group">
                        @Html.LabelFor(p => requerimiento.sku, htmlAttributes: new { @class = "control-label col-xs-4" })

                        <div class="col-xs-8">
                            @Html.TextBoxFor(p => requerimiento.sku, htmlAttributes: new { @class = "form-control" })
                            <span style="margin-right: 20px; font-size:12px; color: darkred; font-weight: normal " class="has-error">*Puede ingresar el SKU del producto que se encuentra en el detalle del pedido.</span>
                        </div>

                    </div>

                </div>

                <!--CODIGO GRUPO CLIENTE-->
                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        <label class="control-label col-xs-4" for="requerimiento_idGrupoCliente">Grupo Cliente:</label>
                        <div class="col-xs-8">
                            @Html.Action("GetGruposCliente", "GrupoCliente",
                    new
                    {
                        grupoClienteSelectId = "requerimiento_idGrupoCliente",
                        selectedValue = requerimiento.idGrupoCliente
                    })
                        </div>
                        @if (requerimiento.usuario.buscaSedesGrupoCliente)
                        {
                            <label class="radio-label" style="margin-left: 15px;margin-top: 5px;">
                                @if (requerimiento.buscarSedesGrupoCliente)
                                {
                                    <input checked type="checkbox" name="requerimiento_buscarSedesGrupoCliente" id="requerimiento_buscarSedesGrupoCliente" value="1">
                                }
                                else
                                {
                                    <input type="checkbox" name="requerimiento_buscarSedesGrupoCliente" id="requerimiento_buscarSedesGrupoCliente" value="1">
                                }
                                Todas las sedes
                            </label>
                        }
                    </div>
                </div>
            </div>
        }

        <div hidden="hidden" class="row">

            <!--FECHA CREACIÓN-->
            <div class="col-sm-6 col-lg-4">
                <label for="requerimiento_fechaCreacionDesde" class="control-label col-xs-6 col-lg-4">Fecha de Creación:</label>
                <div class="input-group col-xs-6 col-lg-8">
                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Desde:</span>
                    </div>
                    <input type="hidden" id="fechaCreacionDesdetmp" value="@ViewBag.fechaCreacionDesde" />
                    <input class="form-control" type="text" id="requerimiento_fechaCreacionDesde" name="requerimiento_fechaCreacionDesde">
                </div>
                <label for="fechaSolicitudHasta" class="control-label col-xs-6 col-lg-4"></label>
                <div class="input-group col-xs-6 col-lg-8">

                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Hasta:</span>
                    </div>
                    <input type="hidden" id="fechaCreacionHastatmp" value="@ViewBag.fechaCreacionHasta" />
                    <input class="form-control" type="text" id="requerimiento_fechaCreacionHasta" name="requerimiento_fechaCreacionHasta">

                </div>
            </div>

            <!--FECHA ENTREGA-->
            <div class="col-sm-6 col-lg-4">
                <label for="requerimiento_fechaEntregaDesde" class="control-label col-xs-6 col-lg-4">Fecha de Entrega:</label>
                <div class="input-group col-xs-6 col-lg-8">
                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Desde:</span>
                    </div>
                    <input type="hidden" id="fechaEntregaDesdetmp" value="@ViewBag.fechaEntregaDesde" />
                    <input class="form-control" type="text" id="requerimiento_fechaEntregaDesde" name="requerimiento_fechaEntregaDesde">
                </div>
                <label for="fechaEntregaHasta" class="control-label col-xs-6 col-lg-4"></label>
                <div class="input-group col-xs-6 col-lg-8">

                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Hasta:</span>
                    </div>
                    <input type="hidden" id="fechaEntregaHastatmp" value="@ViewBag.fechaEntregaHasta" />
                    <input class="form-control" type="text" id="requerimiento_fechaEntregaHasta" name="requerimiento_fechaEntregaHasta">

                </div>
            </div>

            <!--FECHA PROGRACION-->
            <div hidden="hidden" class="col-sm-6 col-lg-4">
                <label for="fechaEntregaHasta" class="control-label col-xs-6 col-lg-4">Fecha de Programación:</label>
                <div class="input-group col-xs-6 col-lg-8">
                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Desde:</span>
                    </div>
                    <input type="hidden" id="fechaProgramacionDesdetmp" value="@ViewBag.fechaProgramacionDesde" />
                    <input class="form-control" type="text" id="requerimiento_fechaProgramacionDesde" name="requerimiento_fechaProgramacionDesde">
                </div>
                <label for="fechaEntregaHasta" class="control-label col-xs-6 col-lg-4"></label>
                <div class="input-group col-xs-6 col-lg-8">

                    <div class="input-group-addon" style="width:70px">
                        <span class="input-group-text" id="basic-addon1">Hasta:</span>
                    </div>
                    <input type="hidden" id="fechaProgramacionHastatmp" value="@ViewBag.fechaProgramacionHasta" />
                    <input class="form-control" type="text" id="requerimiento_fechaProgramacionHasta" name="requerimiento_fechaProgramacionHasta">

                </div>
            </div>

        </div>


      

        <div class="row pull-right" style="margin-bottom:10px; margin-right:0px">
            <button id="btnLimpiarBusquedaPedidos" type="button" class="btn btn-primary  bouton-image botonClean">Limpiar</button>
            <button id="btnBusquedaRequerimientos" type="button" class="btn btn-primary  bouton-image botonSearch">Buscar</button>
        </div>

        <div id="divExportButton" class="row pull-left" style="margin-bottom:10px; margin-left:0px; display: none;">
            <button id="btnExportRequerimientosExcel" type="button" class="btn btn-primary  bouton-image botonExcel" actionLink="@Url.Action("ExportLastSearchRequerimientosExcel", "Pedido")">Exportar</button>
        </div>

        <div class="row">
            <div class="col-sm-6 col-lg-12">
                <h4>Canasta de Productos:</h4>
            </div>
        </div>



        <div class="row">
            <div class="col-sm-6 col-lg-12">
                <table id="tableProductos" class="table">
                    <thead>
                        <tr>
                            <th data-name="producto" data-visible="true">Productos</th>
                            <th data-name="sku" data-visible="true">SKU</th>
                            <th data-name="skuProv" data-visible="true">SKU Prov</th>
                            <th data-name="descripcion" data-visible="true">Descripción</th>
                            <th data-name="unidad" data-visible="true">Unidad</th>
                        </tr>
                    </thead>
                    <tbody>

                        @{int contador = 1;}
                        @foreach (Model.RequerimientoDetalle requerimientoDetalle in requerimiento.requerimientoDetalleList)
                        {
                            <tr>
                            <td>Prod @contador</td>
                            <td>@requerimientoDetalle.producto.sku</td>
                            <td>@requerimientoDetalle.producto.skuProveedor</td>
                            <td>@requerimientoDetalle.producto.descripcion</td>
                            <td>@requerimientoDetalle.unidad</td>
                            </tr>

                            contador++;
                        }
                      
                    </tbody>
                </table>

            </div>
        </div>


        @{contador = 0;}
        @foreach (Model.RequerimientoDetalle requerimientoDetalle in requerimiento.requerimientoDetalleList)
        {
            string nombreInputProducto = "productosku" + contador;

           <input hidden="hidden"  id=@nombreInputProducto value="@requerimientoDetalle.producto.sku"  />
            contador++;
        }


        <div class="row">
            <div class="col-sm-6 col-lg-12">
                <h4>Requerimientos:</h4>
            </div>
        </div>


        <table id="tableRequerimientos" class="table" data-editing="false" data-paging="true" data-paging-count-format="{CP} de {TP}"
               data-editing-show-text='<span class="fooicon fooicon-pencil" aria-hidden="true"></span> Editar Detalle'
               data-editing-allow-add="false"
               data-editing-allow-delete="false"
               data-editing-allow-edit="false"
               data-sorting="true"
               data-show-toggle="false"
               data-editing-hide-text="Finalizar edición"
               data-editing-add-text="Descartar cambios">
            <thead>
                <tr>
                    <th data-name="idRequerimiento" data-visible="false"></th>
                    <th data-name="numero" data-visible="true">N°</th>
                    <th data-name="sedeMP" data-sortable="false" data-visible="false" data-breakpoints="xs">Sede MP</th>
                    <th data-name="codigo" data-visible="false" data-sortable="false" data-breakpoints="xs">Cod. Cliente</th>
                    <th data-name="razonSocial" data-visible="false" data-sortable="true">Razón Social</th>
                    <th data-name="grupoCliente" data-visible="false" data-sortable="true">Grupo Cliente</th>
                    <th data-name="numeroReferenciaCliente" data-visible="false">O/C N°</th>
                    <th data-name="usuario" data-visible="true">Creado por</th>
                    <th data-name="fechaRegistro" data-sortable="true" data-breakpoints="xs">Fecha Registro</th>
                    <!--<th data-name="fechaSolicitud" data-sortable="true" data-breakpoints="xs">Fecha y Hora Solicitud</th>-->
                    <th data-name="rangoFechaEntrega" data-visible="false" data-sortable="true" data-breakpoints="xs">Rango Fecha Entrega</th>
                    <th data-name="horarioEntrega" data-sortable="true" data-visible="false" data-breakpoints="xs">Horarios Entrega</th>
                    <!--<th data-name="fechaProgramacion" data-sortable="true" data-breakpoints="xs">Programado Para:</th>-->

                    <th data-name="distrito" data-sortable="true" data-breakpoints="xs">Distrito Entrega</th>

                    <!--   <th data-name="aprobador" data-sortable="true" data-breakpoints="xs">Modificado última vez por</th>
                    <th data-name="observacion" data-sortable="true" data-breakpoints="xs">Comentario Estado</th>-->
                    <th data-name="estadoCrediticio" data-sortable="true" data-visible="false" data-breakpoints="xs">Estado Crediticio</th>
                    <th data-name="observaciones" data-sortable="true" data-breakpoints="xs">Obs. Uso Interno</th>


                    @{contador = 1;}
                    @foreach (Model.RequerimientoDetalle requerimientoDetalle in requerimiento.requerimientoDetalleList)
                    {                        
                        <th data-name=@requerimientoDetalle.producto.sku data-sortable="true" data-breakpoints="xs">Prod @contador</th>
                        contador++;
                    }

                    <th data-name="total" data-sortable="true" data-breakpoints="xs">Total (No Incl. IGV)</th>
                    <th data-name="estado" data-sortable="true" data-breakpoints="xs">Estado</th>
                    @if (requerimiento.usuario.confirmaStock)
                    {
                        <th data-name="stockConfirmado" data-visible="false">Stock</th>
                        <th data-name="stockConfirmado" data-visible="false">Stock</th>
                    }
                    else
                    {

                        <th data-name="stockConfirmado" data-visible="false">Stock</th>
                        <th data-name="stockConfirmado" data-visible="false">Stock</th>
                    }
                    <th data-name="topePresupuesto" data-sortable="true" data-breakpoints="xs">Tope Ppto.</th>
                    <th data-name="semaforo" data-sortable="true" data-breakpoints="xs"></th>
                    <th data-name="opciones" data-sortable="true" data-breakpoints="xs"></th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>


        @if (requerimientoList.Count == 0)
        {
            <div id="msgBusquedaSinResultados" style="display:none;" class="col-lg-12">
                <p class="pagination-centered" style=" font-size: medium" align="center">
                    La búsqueda no obtuvo resultados.
                </p>
            </div>
        }
        else
        {

        }

        @if (requerimiento.usuario.idUsuario == Guid.Parse("7D485AFF-AC89-462D-BBB8-0F8ADF8C02F1"))
        {
        <div class="row pull-right" style="margin-bottom:10px; margin-right:0px">
            <button id="btnAprobarSolicitudesPreview" type="button" class="btn btn-success" data-toggle="modal" data-target="#modalAprobarTodosPreview">Aprobar Todos</button>
        </div>
        }

    </form>
</div>


