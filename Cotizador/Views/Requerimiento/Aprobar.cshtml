                    <div hidden="hidden" class="row" id="divReferenciaCliente2">
                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(p => requerimiento.numeroReferenciaCliente, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="verNumeroReferenciaCliente"></p>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(p => requerimiento.idRequerimiento, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="verNumeroRequerimiento"></p>
                                </div>
                            </div>
