                            <div class="form-group">
                                @Html.LabelFor(p => requerimiento.usuarioCreacion, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="verUsuarioCreacion"></p>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6 col-lg-4">
                            <div class="form-group">
                                @Html.LabelFor(p => requerimiento.fechaHoraRegistro, htmlAttributes: new { @class = "control-label col-xs-4" })
                                <div class="col-xs-8">
                                    <p class="form-control-static" id="verFechaHoraRegistro"></p>
                                </div>
                            </div>
                        </div>
