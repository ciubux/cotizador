@model List<Model.ArchivoAdjunto>

<div class="row">
    <div class="col-sm-12 col-lg-12">
        <div class="form-group">
            <label for="archivoAdjunto" class="control-label col-xs-2">Adjuntos:</label>
            <div class="col-xs-10">
                <div class="upload-btn-wrapper">                   
                    <input type="hidden" id="origenArchivoAdjunto" value="@ViewBag.origen">
                    <label for="fileUpload" class="btn btn-primary">Adjuntar Archivo(s)</label>
                    <input type="file" id="fileUpload" style="display:none;" />


                    <script type="text/javascript">

                        window.onload = function ()
                        {
                            archivosEncontrados(@Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model,
                            Newtonsoft.Json.Formatting.Indented)), "@ViewBag.origen");
                        };
                    </script>


                </div>
            </div>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-sm-12 col-lg-12">
        <div class="form-group">
            <label for="archivoAdjunto_@ViewBag.origen" class="control-label col-xs-2"></label>
            <div class="col-xs-10">
                <ul id="nombreArchivos_@ViewBag.origen"></ul>
            </div>
        </div>
    </div>
</div>
