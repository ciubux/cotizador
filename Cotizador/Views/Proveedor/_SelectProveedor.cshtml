@model IEnumerable<Model.Proveedor>

@Html.DropDownList("proveedor",
new SelectList(Model, "codigo", "nombre", ViewBag.selectProveedor),
new { @class = "form-control", onchange = @"
$.ajax({
url: '/Proveedor/ChangeProveedor',
type: 'POST',
dataType: 'JSON',
data: {
    proveedor: this.value
}
});
" })