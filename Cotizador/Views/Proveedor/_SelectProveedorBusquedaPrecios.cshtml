@model IEnumerable<Model.Proveedor>

@Html.DropDownList("proveedorBusquedaPrecios",
new SelectList(Model, "nombre", "nombre", this.Session["proveedor"]),
new { @class = "form-control", onchange = @"
$.ajax({
url: '/Home/updateProveedor',
type: 'POST',
dataType: 'JSON',
data: {
    proveedor: this.value
}
});
" })