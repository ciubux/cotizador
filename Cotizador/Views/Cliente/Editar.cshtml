@{
    ViewBag.Title = "Crear/Modificar Cliente";
    Model.Cliente cliente = ViewBag.cliente;
    var disabledLabel = "disabled = ''";
    /*Model.DireccionEntrega direccionEntrega = ViewBag.direccionEntrega;*/
    // Model.pedido.OpcionesConsiderarCantidades opcionesConsiderarCantidades;
    // @model Enum;

    string displaySubDistribuidor = "";
    string disabledSunat = "";
    string displayCanalMultiregional = "";
    string displaySearchSunat = "display: none;";

    string hideNegociacionGrupal = "";
    if (cliente.grupoCliente == null || cliente.grupoCliente.idGrupoCliente == 0)
    {
        hideNegociacionGrupal = "display: none;";
    }

    if (!cliente.esSubDistribuidor)
    {
        displaySubDistribuidor = "display: none;";
    }

    if (!cliente.perteneceCanalMultiregional)
    {
        displayCanalMultiregional = "display: none;";
    }

    if (ViewBag.esRuc)
    {

        displaySearchSunat = "";
        /*
                if (cliente.razonSocialSunat == null || cliente.razonSocialSunat.Trim() == "")
                {
                    disabledSunat = "disabled=\"disabled\"";
                    displaySearchSunat = "display: none;";
                }*/
    }

    string displayChrAsesor = "";
    if (cliente.hoyEsFechaRegistro || cliente.responsableComercial.idVendedor.ToString().Equals(cliente.chrAsesor.preValor)) {
        displayChrAsesor = "display:none;";
    }

    string displayChrSupervisor = "";
    if (cliente.hoyEsFechaRegistro || cliente.supervisorComercial.idVendedor.ToString().Equals(cliente.chrSupervisor.preValor)) {
        displayChrSupervisor = "display:none;";
    }

    string displayChrAsistente = "";
    if (cliente.hoyEsFechaRegistro || cliente.asistenteServicioCliente.idVendedor.ToString().Equals(cliente.chrAsistente.preValor)) {
        displayChrAsistente = "display:none;";
    }

    string solicitarCHRForm = "1";
    if (cliente.hoyEsFechaRegistro)
    {
        solicitarCHRForm = "0";
    }

    if (cliente.esClienteLite && cliente.tipoDocumento == null)
    {
        cliente.ruc = "";
    }
}

<div class="page-header">
    <h3>@ViewBag.Title</h3>
</div>
<div class="container">
    <input type="hidden" id="pagina" value="@ViewBag.pagina" />
    <input type="hidden" id="bloqueaFiltroAsesor" value="0" />
    <input type="hidden" value="@ViewBag.debug" />
    <input type="hidden" id="fechaRegistro" value='@cliente.FechaRegistro.ToString("dd/MM/yyyy")' />
    <input type="hidden" id="esClienteLite" value='@cliente.clienteLiteValue.ToString()' />

    <form class="form-horizontal" enctype="multipart/form-data" id="formularioConArchivos" method="post">
        <input type="hidden" id="considerarCantidades" name="considerarCantidades" value="2" />

        <div class="row">

            @if (cliente.codigo != null && cliente.codigo.Length > 0)
            {
                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        @Html.LabelFor(c => cliente.ciudad, htmlAttributes: new { @class = "control-label col-xs-4", @for = "idCiudad" })
                        <div class="col-xs-8">
                            @Html.Action("GetCiudades", "Ciudad",
                                   new
                                   {
                                       ciudadSelectId = "idCiudad",
                                       selectedValue = cliente.ciudad.idCiudad,
                                       disabled = "disabled"
                                   })
                        </div>
                    </div>
                </div>
            }
            else
            {
                <div class="col-sm-6 col-lg-4">
                    <div class="form-group">
                        @Html.LabelFor(c => cliente.ciudad, htmlAttributes: new { @class = "control-label col-xs-4", @for = "idCiudad" })
                        <div class="col-xs-8">
                            @Html.Action("GetCiudades", "Ciudad",
                                           new
                                           {
                                               ciudadSelectId = "idCiudad",
                                               selectedValue = cliente.ciudad.idCiudad
                                           })
                        </div>
                    </div>
                </div>
            }

            <input type="hidden" id="idCliente" name="idCliente" value="@cliente.idCliente" />


            <!--CODIGO CLIENTE-->
            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(c => cliente.codigo, htmlAttributes: new { @class = "control-label col-xs-4" })
                    <div class="col-xs-8">
                        @Html.TextBoxFor(c => cliente.codigo, htmlAttributes: new { @class = "form-control", @disabled = "disabled" })
                    </div>
                </div>
            </div>

            <!--CODIGO GRUPO CLIENTE-->
            <div class="col-sm-6 col-lg-4">
                @if (cliente.modificaGrupo == 1)
                {
                    <div class="form-group">
                        @Html.LabelFor(c => cliente.grupoCliente, htmlAttributes: new { @class = "control-label col-xs-4" })
                        <div class="col-xs-8">

                            @*<p class="form-control-static" id="verGrupoCliente"></p>*@

                            @Html.Action("GetGruposCliente", "GrupoCliente",
                                        new
                                        {
                                            grupoClienteSelectId = "idGrupoCliente",
                                            selectedValue = cliente.grupoCliente.idGrupoCliente
                                        })
                        </div>
                        <label class="radio-label" style="margin-left: 15px; margin-top: 5px; @hideNegociacionGrupal">
                            @if (cliente.habilitadoNegociacionGrupal)
                            {
                                <input checked type="checkbox" name="cliente_habilitadoNegociacionGrupal" id="cliente_habilitadoNegociacionGrupal" value="1">
                            }
                            else
                            {
                                <input type="checkbox" name="cliente_habilitadoNegociacionGrupal" id="cliente_habilitadoNegociacionGrupal" value="1">
                            }


                            @if (cliente.codigo != String.Empty)
                            {
                                <span>Este Cliente (@cliente.codigo) hereda los precios del grupo.</span>
                            }
                            else
                            {
                                <span>Este Cliente heredará los precios del grupo.</span>
                            }

                        </label>
                    </div>
                }
                else
                {
                    <div class="form-group">
                        <label for="cliente_grupo" class="control-label col-xs-4">Grupo Cliente:</label>
                        <div class="col-xs-6">
                            <p class="form-control-static" id="verGrupoCliente">@cliente.grupoCliente.nombre</p>
                        </div>
                        <label id="verHabilitadoNegociacionGrupal" class="radio-label" style="font-size:12px; color:forestgreen; margin-left: 15px;margin-top: 5px;">
                            <span>Este Cliente (@cliente.codigo) hereda los precios del grupo.</span>
                        </label>
                    </div>
                }
            </div>
        </div>

        <div class="row">

            <!--Tipo Documento Identidad-->
            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(dv => cliente.tipoDocumentoIdentidad, htmlAttributes: new { @class = "control-label col-xs-4 col-lg-4" })
                    <div class="col-xs-8">
                        @Html.DropDownList("tipoDocumentoIdentidad",
                         EnumHelper.GetSelectList(typeof(Model.DocumentoVenta.TiposDocumentoIdentidad), cliente.tipoDocumentoIdentidad),
                         new { @class = "form-control" }
                         )
                    </div>
                </div>
            </div>

            <!--Número Documento-->
            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    <label for="cliente_ruc" class="control-label col-xs-4">Número. Documento:</label>
                    <div class="col-xs-5">
                        @Html.TextBoxFor(c => cliente.ruc, htmlAttributes: new { @class = "form-control" })

                    </div>
                    <div class="col-xs-3 row">
                        <button id="btnRecuperarDatosSunat" type="button" class="btn btn-primary"><img src="~/images/logo_sunat.svg" style="height: 18px; background-color: #ffffff;" /></button>
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-lg-4">
                <div class="form-group">
                    @Html.LabelFor(c => cliente.origen, htmlAttributes: new { @class = "control-label col-xs-4 col-sm-4 col-lg-4", @for = "idOrigen" })
                    <div class="col-xs-8 col-sm-8 col-lg-8">
                        @Html.Action("GetOrigenes", "Origen",
                                new
                                {
                                    origenSelectId = "idOrigen",
                                    selectedValue = cliente.origen.idOrigen,
                                })
                    </div>
                </div>
            </div>

        </div>
        <br />

