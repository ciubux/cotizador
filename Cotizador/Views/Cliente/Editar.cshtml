                            </div>
                        </div>
                    </div>



                    <div class="col-sm-6 col-lg-4">
                        @*<div class="form-group">
                                @Html.LabelFor(c => cliente.contacto1, htmlAttributes: new { @class = "control-label col-xs-4 col-sm-4 col-lg-4" })
