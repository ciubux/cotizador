                                <div class="col-xs-8 col-sm-8 col-lg-8">
                                    @Html.TextBoxFor(c => cliente.emailContacto1, htmlAttributes: new { @class = "form-control" })
                                </div>
                            </div>*@
                    </div>
                </div>


            </fieldset>

            <br />
            <fieldset>
                <legend>Crédito y Condiciones de Pago:</legend>
                <div class="row">
                    <div class="col-sm-6 col-lg-4">
                        <div class="form-group">
                            @Html.LabelFor(dv => cliente.plazoCreditoSolicitado, htmlAttributes: new { @class = "control-label col-xs-4 col-lg-4" })
                            <div class="col-xs-8">
                                @if (cliente.usuario.apruebaPlazoCredito)
                                {
                                    @Html.DropDownList("plazoCreditoSolicitado",
                                                   EnumHelper.GetSelectList(typeof(Model.DocumentoVenta.TipoPago), cliente.plazoCreditoSolicitado),
                                                   new { @class = "form-control", @disabled = "disabled" }
                                                   )
                                }
                                else
                                {
                                    @Html.DropDownList("plazoCreditoSolicitado",
                                                   EnumHelper.GetSelectList(typeof(Model.DocumentoVenta.TipoPago), cliente.plazoCreditoSolicitado),
