                        <div class="form-group">
                            @Html.LabelFor(p => cliente.observaciones, htmlAttributes: new { @class = "control-label  col-xs-4  col-sm-4 col-lg-2" })
                            <div class="col-xs-8 col-sm-8 col-lg-10">
                                @Html.TextAreaFor(p => cliente.observaciones, htmlAttributes: new { @class = "form-control", rows = "6", cols = "300", style = "min-width: 100%" })
                            </div>
                        </div>
                    </div>

                    <div class="col-sm-6 col-lg-4">
                        @*<div class="form-group">
                                @Html.LabelFor(c => cliente.telefonoContacto1, htmlAttributes: new { @class = "control-label col-xs-4 col-sm-4 col-lg-4" })
                                <div class="col-xs-8 col-sm-8 col-lg-8">
