@{
    ViewBag.Title = "Crear/Modificar Nota Ingreso";
    Model.GuiaRemision guiaRemision = ViewBag.guiaRemision;
    Model.Transportista transportista = ViewBag.transportista;
}

<div class="page-header">
    <h3>@ViewBag.Title</h3>
</div>
<div class="container">

    <input type="hidden" id="respuestaConfirmacion" />
    <input type="hidden" id="pagina" value="@ViewBag.pagina" />
    <input type="hidden" value="@ViewBag.debug" />
    <form class="form-horizontal">
        <div class="col-lg-12">
            <p class="pagination-centered" style=" font-size: medium" align="center">
                No tiene abierta ninguna nota de ingreso, ingrese a "Búsqueda Pedidos" y proceda a buscar los pedidos ingresados para poder ingresarlos.
            </p>
        </div>

    </form>
</div>







@section Scripts {
    @Scripts.Render("~/bundles/notaIngreso")
}



